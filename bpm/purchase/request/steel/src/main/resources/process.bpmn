      <bpmn2:incoming>SequenceFlow_7</bpmn2:incoming>
      <bpmn2:incoming>SequenceFlow_10</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_9</bpmn2:outgoing>
      <bpmn2:startEvent id="StartEvent_1">
        <bpmn2:outgoing>SequenceFlow_3</bpmn2:outgoing>
      </bpmn2:startEvent>
      <bpmn2:sequenceFlow id="SequenceFlow_3" name="" sourceRef="StartEvent_1" targetRef="QuotationUserTask"/>
      <bpmn2:userTask id="QuotationUserTask" camunda:assignee="demo" camunda:formKey="app:quotation.xhtml" name="Cotação">
        <bpmn2:incoming>SequenceFlow_3</bpmn2:incoming>
        <bpmn2:outgoing>SequenceFlow_11</bpmn2:outgoing>
      </bpmn2:userTask>
      <bpmn2:sequenceFlow id="SequenceFlow_11" name="" sourceRef="QuotationUserTask" targetRef="PurchasePlanningUserTask"/>
      <bpmn2:userTask id="PurchasePlanningUserTask" camunda:assignee="demo" camunda:formKey="app:planning.xhtml" name="Planejamento&#xD;&#xA;Compra">
        <bpmn2:incoming>SequenceFlow_11</bpmn2:incoming>
        <bpmn2:outgoing>SequenceFlow_4</bpmn2:outgoing>
      </bpmn2:userTask>
      <bpmn2:sequenceFlow id="SequenceFlow_4" name="" sourceRef="PurchasePlanningUserTask" targetRef="EndEvent_1"/>
      <bpmn2:endEvent id="EndEvent_1">
        <bpmn2:incoming>SequenceFlow_4</bpmn2:incoming>
      </bpmn2:endEvent>
    </bpmn2:subProcess>
    <bpmn2:sequenceFlow id="SequenceFlow_9" name="" sourceRef="PurchasingPlanningSubProcess" targetRef="GeneratePurchaseOrdersServiceTask"/>
    <bpmn2:serviceTask id="GeneratePurchaseOrdersServiceTask" camunda:delegateExpression="${Logger}" camunda:async="true" name="Gerar&#xD;&#xA;Pedidos de &#xD;&#xA;Compra">
      <bpmn2:extensionElements>
        <camunda:executionListener delegateExpression="${GenerateOrders}" event="start"/>
        <fox:failedJobRetryTimeCycle>R3/PT10M</fox:failedJobRetryTimeCycle>
      </bpmn2:extensionElements>
      <bpmn2:incoming>SequenceFlow_9</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_5</bpmn2:outgoing>
    </bpmn2:serviceTask>
    <bpmn2:sequenceFlow id="SequenceFlow_5" name="" sourceRef="GeneratePurchaseOrdersServiceTask" targetRef="ProcessPurchaseOrdersCallActivity"/>
    <bpmn2:endEvent id="SteelPurchaseEndEvent" name="Requisições Processadas&#xD;&#xA;Pedidos Efetivados">
      <bpmn2:incoming>SequenceFlow_6</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:callActivity id="ProcessPurchaseOrdersCallActivity" name="Processar &#xD;&#xA;Pedidos" calledElement="br.com.altamira.bpm.purchasing.request.ProcessPurchaseOrders">
      <bpmn2:extensionElements>
        <camunda:in source="orderId" target="orderId"/>
        <camunda:in source="loopCounter" target="loopCounter"/>
      </bpmn2:extensionElements>
      <bpmn2:incoming>SequenceFlow_5</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_6</bpmn2:outgoing>
      <bpmn2:multiInstanceLoopCharacteristics camunda:collection="ordersList" camunda:elementVariable="orderId"/>
    </bpmn2:callActivity>
    <bpmn2:sequenceFlow id="SequenceFlow_6" name="" sourceRef="ProcessPurchaseOrdersCallActivity" targetRef="SteelPurchaseEndEvent"/>
    <bpmn2:textAnnotation id="TextAnnotation_2">
      <bpmn2:text>Caso exista um processo de compra em andamento, acrescenta a nova requisição de compra ao processo atual</bpmn2:text>
    </bpmn2:textAnnotation>
    <bpmn2:association id="Association_6" sourceRef="TextAnnotation_2" targetRef="IncludeRequestBoundaryEvent"/>
  </bpmn2:process>
