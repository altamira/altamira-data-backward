      <bpmn2:incoming>SequenceFlow_44</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_45</bpmn2:outgoing>
    </bpmn2:userTask>
    <bpmn2:sequenceFlow id="SequenceFlow_45" name="" sourceRef="UpdateSupplierContactInfoUserTask" targetRef="SendPurchaseOrderSendTask"/>
    <bpmn2:parallelGateway id="ParallelGateway_1">
      <bpmn2:incoming>SequenceFlow_18</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_19</bpmn2:outgoing>
      <bpmn2:outgoing>SequenceFlow_20</bpmn2:outgoing>
    </bpmn2:parallelGateway>
    <bpmn2:sequenceFlow id="SequenceFlow_19" name="" sourceRef="ParallelGateway_1" targetRef="ConfirmOrderReceivedUserTask"/>
    <bpmn2:sequenceFlow id="SequenceFlow_20" name="" sourceRef="ParallelGateway_1" targetRef="IntermediateCatchEvent"/>
    <bpmn2:intermediateCatchEvent id="IntermediateCatchEvent">
      <bpmn2:incoming>SequenceFlow_20</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_13</bpmn2:outgoing>
      <bpmn2:messageEventDefinition id="_MessageEventDefinition_6" messageRef="Message_4"/>
    </bpmn2:intermediateCatchEvent>
    <bpmn2:boundaryEvent id="WaitingForConfirmationBoundaryEvent" name="2 dias &#xD;&#xA;uteis" attachedToRef="ConfirmOrderReceivedUserTask" cancelActivity="false">
      <bpmn2:extensionElements/>
      <bpmn2:outgoing>SequenceFlow_24</bpmn2:outgoing>
      <bpmn2:timerEventDefinition id="_TimerEventDefinition_2">
        <bpmn2:timeCycle xsi:type="bpmn2:tFormalExpression">R3/PT1M</bpmn2:timeCycle>
      </bpmn2:timerEventDefinition>
    </bpmn2:boundaryEvent>
    <bpmn2:sequenceFlow id="SequenceFlow_24" name="" sourceRef="WaitingForConfirmationBoundaryEvent" targetRef="NotifyPurchaseManagerServiceTask"/>
    <bpmn2:userTask id="ConfirmOrderReceivedUserTask" camunda:assignee="demo" name="Confirmar&#xD;&#xA;Recebimento">
      <bpmn2:extensionElements>
        <camunda:formData>
          <camunda:formField id="status" label="Situacao do Pedido" type="enum">
            <camunda:value id="accepted" name="Confirmado"/>
            <camunda:value id="not_accepted" name="Nao Aceitou"/>
            <camunda:value id="not_received" name="Nao Recebeu"/>
          </camunda:formField>
        </camunda:formData>
      </bpmn2:extensionElements>
      <bpmn2:incoming>SequenceFlow_19</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_17</bpmn2:outgoing>
    </bpmn2:userTask>
    <bpmn2:sendTask id="NotifyPurchaseManagerServiceTask" camunda:delegateExpression="${NotifyOrderConfirmationPending}" name="Notificar &#xD;&#xA;Responsável">
      <bpmn2:incoming>SequenceFlow_24</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_31</bpmn2:outgoing>
    </bpmn2:sendTask>
    <bpmn2:sequenceFlow id="SequenceFlow_31" name="" sourceRef="NotifyPurchaseManagerServiceTask" targetRef="OrderPendingEndEvent"/>
    <bpmn2:exclusiveGateway id="CheckStatusExclusiveGateway" default="OrderConfirmedSequenceFlow">
      <bpmn2:incoming>SequenceFlow_13</bpmn2:incoming>
      <bpmn2:incoming>SequenceFlow_17</bpmn2:incoming>
      <bpmn2:outgoing>OrderConfirmedSequenceFlow</bpmn2:outgoing>
      <bpmn2:outgoing>RequestResendSequenceFlow</bpmn2:outgoing>
      <bpmn2:outgoing>ResendOrderSequenceFlow</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    <bpmn2:sequenceFlow id="OrderConfirmedSequenceFlow" name="" sourceRef="CheckStatusExclusiveGateway" targetRef="OrderSubmitedEndEvent"/>
    <bpmn2:sequenceFlow id="RequestResendSequenceFlow" name="" sourceRef="CheckStatusExclusiveGateway" targetRef="ResendRequestEndEvent">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression"><![CDATA[#{status=="not_accepted"}]]></bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    <bpmn2:sequenceFlow id="ResendOrderSequenceFlow" name="" sourceRef="CheckStatusExclusiveGateway" targetRef="OrderResendIntermediateThrowEvent">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression"><![CDATA[#{status=="not_received"}]]></bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    <bpmn2:endEvent id="OrderSubmitedEndEvent" name="Pedido&#xD;&#xA;Efetivado">
      <bpmn2:incoming>OrderConfirmedSequenceFlow</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:intermediateThrowEvent id="OrderResendIntermediateThrowEvent" name="Reenviar&#xD;&#xA;Pedido">
      <bpmn2:incoming>ResendOrderSequenceFlow</bpmn2:incoming>
      <bpmn2:linkEventDefinition id="ResendOrderLinkEventDefinitionThrow" name="Reenviar&#xD;&#xA;Pedido"/>
    </bpmn2:intermediateThrowEvent>
    <bpmn2:endEvent id="OrderPendingEndEvent" name="Pedido Pendente&#xD;&#xA;Aguardando confirmação">
      <bpmn2:incoming>SequenceFlow_31</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:endEvent id="ResendRequestEndEvent" name="Gerar Nova&#xD;&#xA;Requisição &#xD;&#xA;Parcial ou Total">
      <bpmn2:incoming>RequestResendSequenceFlow</bpmn2:incoming>
      <bpmn2:messageEventDefinition id="_MessageEventDefinition_9" camunda:delegateExpression="${Logger}"/>
    </bpmn2:endEvent>
    <bpmn2:sequenceFlow id="SequenceFlow_13" name="" sourceRef="IntermediateCatchEvent" targetRef="CheckStatusExclusiveGateway"/>
    <bpmn2:sequenceFlow id="SequenceFlow_17" name="" sourceRef="ConfirmOrderReceivedUserTask" targetRef="CheckStatusExclusiveGateway"/>
  </bpmn2:process>
