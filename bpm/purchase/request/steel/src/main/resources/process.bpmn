      <bpmn2:incoming>SequenceFlow_27</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_32</bpmn2:outgoing>
    </bpmn2:userTask>
    <bpmn2:sequenceFlow id="SequenceFlow_27" name="" sourceRef="UserTask_3" targetRef="UserTask_4"/>
    <bpmn2:endEvent id="EndEvent_6" name="Informar o&#xD;&#xA;Cliente">
      <bpmn2:incoming>SequenceFlow_32</bpmn2:incoming>
      <bpmn2:messageEventDefinition id="_MessageEventDefinition_8"/>
    </bpmn2:endEvent>
    <bpmn2:sequenceFlow id="SequenceFlow_32" name="" sourceRef="UserTask_4" targetRef="EndEvent_6"/>
  </bpmn2:process>
  <bpmn2:message id="Message_4" name="ORDER_RECEIVED_MESSAGE"/>
  <bpmn2:process id="br.com.altamira.bpm.purchasing.request.ProcessPurchaseOrders" name="Processar Pedidos" isExecutable="true">
    <bpmn2:laneSet id="LaneSet_5" name="Lane Set 5">
      <bpmn2:lane id="ProcessPurchaseOrdersLane" name="Processar Pedidos">
        <bpmn2:flowNodeRef>StartEvent_3</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>CheckOutPurchaseOrderUserTask</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>SendPurchaseOrderSendTask</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>OrderResendIntermediateCatchEvent</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>UpdateSupplierContactInfoUserTask</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>ParallelGateway_1</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>IntermediateCatchEvent</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>WaitingForConfirmationBoundaryEvent</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>ConfirmOrderReceivedUserTask</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>NotifyPurchaseManagerServiceTask</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>CheckStatusExclusiveGateway</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>OrderSubmitedEndEvent</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>OrderResendIntermediateThrowEvent</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>OrderPendingEndEvent</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>ResendRequestEndEvent</bpmn2:flowNodeRef>
      </bpmn2:lane>
    </bpmn2:laneSet>
    <bpmn2:startEvent id="StartEvent_3">
      <bpmn2:outgoing>SequenceFlow_14</bpmn2:outgoing>
    </bpmn2:startEvent>
    <bpmn2:sequenceFlow id="SequenceFlow_14" name="" sourceRef="StartEvent_3" targetRef="CheckOutPurchaseOrderUserTask"/>
    <bpmn2:userTask id="CheckOutPurchaseOrderUserTask" camunda:assignee="demo" camunda:candidateGroups="Compras" camunda:formKey="app:edit-order.xhtml" name="Conferir &#xD;&#xA;Pedido de &#xD;&#xA;Compra ${loopCounter}/${orderId}">
      <bpmn2:incoming>SequenceFlow_14</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_8</bpmn2:outgoing>
    </bpmn2:userTask>
    <bpmn2:sequenceFlow id="SequenceFlow_8" name="" sourceRef="CheckOutPurchaseOrderUserTask" targetRef="SendPurchaseOrderSendTask"/>
    <bpmn2:sendTask id="SendPurchaseOrderSendTask" camunda:delegateExpression="${Logger}" name="Enviar&#xD;&#xA;Pedido&#xD;&#xA;Compra">
      <bpmn2:incoming>SequenceFlow_8</bpmn2:incoming>
      <bpmn2:incoming>SequenceFlow_45</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_18</bpmn2:outgoing>
    </bpmn2:sendTask>
    <bpmn2:sequenceFlow id="SequenceFlow_18" name="" sourceRef="SendPurchaseOrderSendTask" targetRef="ParallelGateway_1"/>
    <bpmn2:intermediateCatchEvent id="OrderResendIntermediateCatchEvent" name="Reenviar&#xD;&#xA;Pedido">
      <bpmn2:outgoing>SequenceFlow_44</bpmn2:outgoing>
      <bpmn2:linkEventDefinition id="ResendOrderLinkEventDefinitionCatch" name="Reenviar&#xD;&#xA;Pedido"/>
    </bpmn2:intermediateCatchEvent>
    <bpmn2:sequenceFlow id="SequenceFlow_44" name="" sourceRef="OrderResendIntermediateCatchEvent" targetRef="UpdateSupplierContactInfoUserTask"/>
