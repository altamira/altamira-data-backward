        <bpmn2:flowNodeRef>IncludeRequestBoundaryEvent</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>PurchasingPlanningSubProcess</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>GeneratePurchaseOrdersServiceTask</bpmn2:flowNodeRef>
